{"ast":null,"code":"//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport MenuBar from \"../components/MenuBar.vue\";\nimport 'animate.css';\nexport default {\n  name: \"Home\",\n  components: {\n    MenuBar: MenuBar\n  },\n  data: function data() {\n    return {\n      bannerList: [\"one\", \"tow\"],\n      ind: 0,\n      canDoIt: true\n    };\n  },\n  beforeCreate: function beforeCreate() {\n    this.$store.commit(\"isLogin\");\n  },\n  methods: {\n    test: function test(i) {\n      console.log(this.bannerList[i]);\n    },\n    // 鼠标滚轮切换路由\n    changeBanner: function changeBanner(e) {\n      var evt = e || window.event;\n\n      if (evt.deltaY > 0) {\n        if (this.ind < this.bannerList.length) {\n          this.$router.push(this.bannerList[this.ind]);\n          this.ind++;\n        }\n      } else {\n        if (this.ind > 0) {\n          window.history.back();\n          this.ind--;\n        }\n      }\n    },\n    // 防抖函数\n    changeEvent: function changeEvent(e) {\n      var _this = this;\n\n      if (this.canDoIt) this.changeBanner(e);\n      this.canDoIt = false;\n      setTimeout(function () {\n        _this.canDoIt = true;\n      }, 500);\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;AAsBA;AACA;AACA;AAEAA,cAFA;AAGAC;AACAC;AADA,GAHA;AAMAC,MANA,kBAMA;AACA;AACAC,gCADA;AAEAC,YAFA;AAGAC;AAHA;AAKA,GAZA;AAaAC;AACA;AACA,GAfA;AAgBAC;AACAC,QADA,gBACAC,CADA,EACA;AACAC;AACA,KAHA;AAIA;AACAC,gBALA,wBAKAC,CALA,EAKA;AACA;;AACA;AACA;AACA;AACA;AACA;AACA,OALA,MAKA;AACA;AACAC;AACA;AACA;AACA;AAEA,KAnBA;AAoBA;AACAC,eArBA,uBAqBAF,CArBA,EAqBA;AAAA;;AACA;AACA;AACAG;AACA;AACA,OAFA,EAEA,GAFA;AAGA;AA3BA;AAhBA","names":["name","components","MenuBar","data","bannerList","ind","canDoIt","beforeCreate","methods","test","i","console","changeBanner","e","window","changeEvent","setTimeout"],"sourceRoot":"src/view","sources":["HomePage.vue"],"sourcesContent":["<template>\r\n  <div class=\"content\" @mousewheel=\"changeEvent\">\