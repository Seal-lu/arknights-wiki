:["Outfit.vue"],"sourcesContent":["<template>\r\n    <div class=\"outfit\">\r\n        <div class=\"outfit_left\">\r\n            <!-- 三角背景 -->\r\n            <div class=\"top_left\"></div>\r\n            <div class=\"top_right\"></div>\r\n            <!-- 半身像和头像 -->\r\n            <div>\r\n                <div class=\"header\">\r\n                    <div>\r\n                        <img :src=\"target.half\" alt=\"\">\r\n                    </div>\r\n                </div>\r\n                <div class=\"half\">\r\n                    <div>\r\n                        <img :src=\"target.half\" alt=\"\">\r\n                    </div>\r\n                </div>\r\n                <div class=\"info\">\r\n                    <p id=\"title\">{{outfit.logoName}}</p>\r\n                    <p id=\"new\">NEW APPIVAL</p>\r\n                    <div class=\"witer\">\r\n                        <p><i class=\"el-icon-user\"></i></p>\r\n                        <p><i class=\"el-icon-edit\"></i></p>\r\n                    </div>\r\n                    <p id=\"description\">{{target.description}}</p>\r\n                </div>\r\n            </div>\r\n            <!-- info -->\r\n            \r\n            <img :src=\"outfit.logo\" alt=\"\" class=\"logo\">\r\n            <img :src=\"target.outfit\" alt=\"\" class=\"target\">\r\n        </div>\r\n        <div class=\"outfit_right\">\r\n            <div class=\"logoName\">\r\n                <div>\r\n                    <p>#OUTFIT OF THE DAY</p>\r\n                    <p>{{outfit.type}}</p>\r\n                </div>\r\n                <div></div>\r\n            </div>\r\n            <div class=\"swiper\">\r\n                <el-carousel \r\n                    type=\"card\" \r\n                    height=\"280px\" \r\n                    :autoplay='false' \r\n                    arrow='never'\r\n                    :initial-index='initInd'\r\n                    indicator-position='none'\r\n                    :loop='false'\r\n                    @change=\"changeInd\"\r\n                >\r\n                    <el-carousel-item v-for=\"item in outfit.current\" :key=\"item.id\">\r\n                        <img class=\"medium\" :src=\"item.half\" alt=\"\">\r\n                    </el-carousel-item>\r\n                </el-carousel>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    name:'outfit',\r\n    data(){\r\n        return{\r\n            outfit:{},\r\n            target:{},\r\n            initInd:0\r\n        }\r\n    },\r\n    created(){\r\n        this.getData()\r\n    },\r\n    updated(){\r\n        console.log('was updated');\r\n        this.$router.replace('/outfit/'+this.target.id)\r\n    },\r\n    methods:{\r\n        getData(){\r\n            let path = this.$route.params.id\r\n            let type = this.$route.params.type\r\n            this.$store.state.outfitsList.forEach(item => {\r\n                item.type == type? this.outfit = item : {}\r\n                item.current.forEach((ele,ind) => {\r\n                    if(ele.id == path){\r\n                        this.target = ele\r\n                        this.initInd = ind\r\n                    }else\r\n                        return this.target\r\n\r\n                });\r\n            });\r\n        },\r\n        changeInd(ind){\r\n            this.target = this.outfit.current[ind]\r\n        }\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n\r\n<style lang=\"scss\" scoped>\r\n    .outfit{\r\n        display: flex;\r\n        background: url('../../public/img/bg_inner.3d1e0e7c.png');\r\n        width: 100%;\r\n        height: 92vh;\r\n        overflow: hidden;\r\n        .outfit_left{\r\n            .info{\r\n                font-size: 12px;\r\n                padding: 5px 10px;\r\n                box-sizing: border-box;\r\n                top: 60%;\r\n                z-index: 1001;\r\n                color: white;\r\n                width: 200px;\r\n                position: absolute;\r\n                right: 2%;\r\n                background: rgb(18, 42, 18);\r\n                #title{\r\n                    font-weight: bold;\r\n                    font-size: 19px;\r\n                }\r\n                #new{\r\n                    background: black;\r\n                }\r\n                .witer{\r\n                    margin-top: 8px;\r\n                    background: skyblue;\r\n                    padding: 3px;\r\n                    box-sizing: border-box;\r\n                }\r\n                #description{\r\n                    transform: scale(.9);\r\n                }\r\n            }\r\n            .top_left{\r\n                position: absolute;\r\n                background-image: linear-gradient(159deg,rgb(71, 83, 77) 50%,rgb(0, 0, 0,0) 50%);\r\n                top: 0%;\r\n                width: 60%;\r\n                height: 30%;\r\n                z-index: 99;\r\n            }\r\n            .top_right{\r\n                right: 0%;\r\n                position: absolute;\r\n                z-index: 0;\r\n                background-image: linear-gradient(-170deg,rgb(163, 159, 159) 50%,rgb(0, 0, 0,0) 50%);\r\n                top: 0%;\r\n                width: 60%;\r\n                height: 28%;\r\n            }\r\n            .header{\r\n                transform: scale(.7);\r\n                z-index: 99;\r\n                position: absolute;\r\n                top: 3%;\r\n                right: 0%;\r\n                width: 100px;\r\n                height: 100px;\r\n                border: 10px solid white;\r\n                z-index: 1000;\r\n                div{\r\n                    position: absolute;\r\n                    top: -10px;\r\n                    left: -10px;\r\n                    width: 120px;\r\n                    height: 120px;\r\n                    overflow: hidden;\r\n                }\r\n                img{\r\n                    position: relative;\r\n                    width: 100%;\r\n                }\r\n            }\r\n            .half{\r\n                @extend .header;\r\n                height: 230px;\r\n                top: 20%;\r\n                div{\r\n                    top: 10px;\r\n                    height: 100%;\r\n                }\r\n            }\r\n            position: relative;\r\n            width: 60%;\r\n            height: 100%;\r\n            .logo{\r\n                position: absolute;\r\n                top:20%;\r\n                left: 5%;\r\n                width: 20%;\r\n                z-index: 999;\r\n            }\r\n            .target{\r\n                z-index: 1000;\r\n                position: relative;\r\n                left: 10%;\r\n                height: 100%;\r\n            }\r\n        }\r\n        .outfit_right{\r\n            display: flex;\r\n            flex-direction: column;\r\n            color: white;\r\n            width: 40%;\r\n            height: 100%;\r\n            .logoName{\r\n                margin: 0% auto;\r\n                width: 140px;\r\n                div:first-child{\r\n                    background: green;\r\n                    height: 50px;\r\n                    display: flex;\r\n                    justify-content: space-around;\r\n                    flex-direction: column;\r\n                    align-items: center;\r\n                    :first-child{\r\n                        text-align: center;\r\n                        white-space: nowrap;\r\n                        font-size: 12px;\r\n                        transform: scale(.6);\r\n                    }\r\n                }\r\n            }\r\n            .swiper{\r\n                width: 100%;\r\n            }\r\n        }\r\n    }\r\n</style>\r\n<style lang=\"scss\">\r\n    .el-carousel{\r\n        width: 60%;\r\n        margin: 0% auto;\r\n    }\r\n    .el-carousel__item{\r\n        background: black;\r\n        width: 140px;\r\n        height: 100%;\r\n        img{\r\n            height: 